{
    "dev": {
        "app_function": "app.handler",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "lambda-project",
        "runtime": "python3.12",
        "s3_bucket": "dolar-raw-0723",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole"
        "timeout_seconds": 300,
        "events": [{
                "function": "app.handler",
                "expression": "cron(30 4 * * ? *)"
            },
            {
                "function": "app.s3_to_rds_handler",
                "event_source": {
                    "arn": "arn:aws:s3:::dolar-raw-0723",
                    "events": ["s3:ObjectCreated:*"]
                }
            }
        ],
        "environment_variables": {
            "RDS_HOST": "database-1.cvt0hv2uvvou.us-east-1.rds.amazonaws.com",
            "RDS_USER": "jvaldes",
            "RDS_PASSWORD": "Jvaldes_2303",
            "RDS_DB_NAME": "bigdata_parcial"
        }
    }
}